int led=1;
int l1=2;
int l2=3;
int l3=4;
int l4=5;
int l5=6;
int l6=7;
int l7=8;
int l8=9;
int l9=10;
int l10=11;


void setup() {
  pinMode(led,1);
  pinMode(l2,1);
  pinMode(l3,1);
  pinMode(l4,1);
  pinMode(l5,1);
  pinMode(l6,1);
  pinMode(l7,1);
  pinMode(l8,1);
  pinMode(l9,1);
  pinMode(l10,1);
}

void loop() {
  digitalWrite(led,1);
  delay(500);
  digitalWrite(led,0);
  digitalWrite(l1,1);
  delay(500);
  digitalWrite(l1,0);
  digitalWrite(l2,1);
  delay(500);
  digitalWrite(l2,0);
  digitalWrite(l3,1);
  delay(500);
  digitalWrite(l3,0);
  digitalWrite(l4,1);
  delay(500);
  digitalWrite(l4,0);
  digitalWrite(l5,1);
  delay(500);
  digitalWrite(l5,0);
  digitalWrite(l6,1);
  delay(500);
  digitalWrite(l6,0);
  digitalWrite(l7,1);
  delay(500);
  digitalWrite(l7,0);
  digitalWrite(l8,1);
  delay(500);
  digitalWrite(l8,0);
  digitalWrite(l9,1);
  delay(500);
  digitalWrite(l9,0);
  digitalWrite(l10,1);
  
  
  
  // put your main code here, to run repeatedly:

}
